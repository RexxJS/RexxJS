{
  "name": "rexxjs-jq-address",
  "version": "1.0.0",
  "description": "jq ADDRESS library for RexxJS using jq-wasm",
  "main": "src/jq-address.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development", 
    "build:nodeps": "NODE_ENV=production webpack --config webpack.nodeps.config.js",
    "build:nodeps:dev": "webpack --config webpack.nodeps.config.js --mode=development",
    "build:all": "npm run build && npm run build:nodeps",
    "test": "jest",
    "test:watch": "jest --watch",
    "install:core": "cd ../../../core && npm install",
    "pretest": "npm run install:core"
  },
  "dependencies": {
    "jq-wasm": "1.1.0-jq-1.8.1"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "terser-webpack-plugin": "^5.3.9",
    "buffer": "^6.0.3",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "jest": "^29.7.0"
  },
  "keywords": [
    "rexxjs",
    "jq",
    "json",
    "query",
    "wasm"
  ],
  "author": "RexxJS Project",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.js", "**/*.test.js", "**/*.spec.js"]
  }
}