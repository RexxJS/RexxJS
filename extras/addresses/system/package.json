{
  "name": "rexxjs-system-address",
  "version": "1.0.0",
  "description": "System ADDRESS library for RexxJS - provides OS and system integration",
  "main": "system-address.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "install:core": "cd ../../../core && npm install",
    "pretest": "npm run install:core",
    "build": "webpack --config webpack.bundle.config.js --mode=production",
    "build:dev": "webpack --config webpack.bundle.config.js --mode=development"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "keywords": [
    "rexxjs",
    "system",
    "os",
    "filesystem",
    "process"
  ],
  "author": "RexxJS Project",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/*.test.js",
      "**/*.spec.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "directories": {
    "example": "examples"
  },
  "type": "commonjs",
  "dependencies": {
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4"
  }
}
